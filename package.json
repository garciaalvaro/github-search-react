{
	"name": "github-search-react",
	"version": "1.2.2",
	"description": "Small app to search GitHub repositories",
	"scripts": {
		"start": "npm run dev",
		"dev": "npm run webpack:dev & npm run server:dev",
		"prod": "npm run webpack:prod && npm run server:prod",
		"prod:stop": "npm run server:prod:stop",
		"webpack:dev": "webpack --mode development --watch",
		"webpack:prod": "webpack --mode production",
		"server:dev": "pm2-dev start server.js",
		"server:prod": "pm2 start server.js --name 'github-search-react'",
		"server:prod:stop": "pm2 stop github-search-react",
		"server:docker": "pm2-runtime start server.js --name 'github-search-react'",
		"prestart:docker": "npm run webpack:prod",
		"start:docker": "npm run server:docker",
		"docker": "docker-compose up -d",
		"docker:stop": "docker-compose down",
		"test": "tsc && jest",
		"type-check": "tsc",
		"jest": "jest --watch"
	},
	"dependencies": {
		"express": "^4.17.1",
		"pm2": "^4.4.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-redux": "^7.2.0",
		"redux": "^4.0.5"
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/plugin-transform-runtime": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@babel/register": "^7.10.4",
		"@types/enzyme": "^3.10.5",
		"@types/jest": "^26.0.4",
		"@types/react": "^16.9.42",
		"@types/react-dom": "^16.9.8",
		"@types/react-redux": "^7.1.9",
		"@typescript-eslint/eslint-plugin": "^3.6.0",
		"@typescript-eslint/parser": "^3.6.0",
		"babel-loader": "^8.1.0",
		"clean-css-loader": "^2.0.0",
		"css-loader": "^3.6.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^7.4.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.20.3",
		"html-webpack-plugin": "^4.3.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^26.1.0",
		"jest-environment-enzyme": "^7.1.2",
		"jest-enzyme": "^7.1.2",
		"mini-css-extract-plugin": "^0.9.0",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^2.0.5",
		"redux-devtools-extension": "^2.13.8",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.9.6",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12"
	},
	"main": "server.js",
	"author": "Alvaro Garcia",
	"license": "MIT"
}