{
	"name": "github-search-react",
	"version": "1.2.1",
	"description": "Small app to search GitHub repositories",
	"scripts": {
		"webpack:dev": "webpack --mode development --watch",
		"webpack:prod": "webpack --mode production",
		"server:dev": "pm2-dev start server.js",
		"server:prod": "pm2 start server.js --name 'github-search-react'",
		"server:docker": "pm2-runtime start server.js --name 'github-search-react'",
		"server-stop:prod": "pm2 stop github-search-react",
		"prestart": "npm run webpack:prod",
		"start": "npm run server:prod",
		"prestart:docker": "npm run webpack:prod",
		"start:docker": "npm run server:docker",
		"docker": "docker-compose up -d",
		"docker-stop": "docker-compose down",
		"stop": "npm run server-stop:prod",
		"dev": "npm run webpack:dev & npm run server:dev",
		"test": "tsc && jest",
		"type-check": "tsc",
		"jest": "jest --watch"
	},
	"dependencies": {
		"express": "^4.17.1",
		"pm2": "^4.2.3",
		"react": "^16.13.0",
		"react-dom": "^16.13.0",
		"react-redux": "^7.2.0",
		"redux": "^4.0.5"
	},
	"devDependencies": {
		"@babel/core": "^7.8.7",
		"@babel/plugin-transform-runtime": "^7.8.3",
		"@babel/preset-env": "^7.8.7",
		"@babel/preset-react": "^7.8.3",
		"@babel/preset-typescript": "^7.8.3",
		"@babel/register": "^7.8.6",
		"@types/enzyme": "^3.10.5",
		"@types/jest": "^25.1.4",
		"@types/react": "^16.9.23",
		"@types/react-dom": "^16.9.5",
		"@types/react-redux": "^7.1.7",
		"@typescript-eslint/eslint-plugin": "^2.23.0",
		"@typescript-eslint/parser": "^2.23.0",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.4.2",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.19.0",
		"html-webpack-plugin": "^3.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^25.1.0",
		"jest-environment-enzyme": "^7.1.2",
		"jest-enzyme": "^7.1.2",
		"mini-css-extract-plugin": "^0.9.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^1.19.1",
		"redux-devtools-extension": "^2.13.8",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"terser-webpack-plugin": "^2.3.5",
		"typescript": "^3.8.3",
		"webpack": "^4.42.0",
		"webpack-cli": "^3.3.11"
	},
	"main": "server.js",
	"author": "Alvaro Garcia",
	"license": "MIT"
}
